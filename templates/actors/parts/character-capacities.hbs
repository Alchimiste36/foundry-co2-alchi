   <div class="path-list">
    {{log 'paths' this}}
        <div class="grid">
            <div class="gridrow grid-6col-150 gridheader">
                <div>&nbsp;</div>
                <div class="gridcell gridcell-sm bg-default">1</div>
                <div class="gridcell gridcell-sm bg-default">2</div>
                <div class="gridcell gridcell-sm bg-default">3</div>
                <div class="gridcell gridcell-sm bg-default">4</div>
                <div class="gridcell gridcell-sm bg-default">5</div>
            </div>
            {{#each paths as |path id|}}
              {{log 'path' path id}}
                <div class="gridrow grid-6col-150 item" data-item-id="{{path._id}}" data-item-type="item">
                    <div class="gridcell left gridheader color-default no-border">
                        <a class="item-edit ellipsis" style="width: 100%; text-align: left" title="{{path.name}}">{{path.name}}</a>&nbsp;
                        <span class="path-controls item" data-item-id="{{path._id}}" style="padding-right:5px;"><a class="item-control item-delete" title="{{localize "CO.label.delete"}}"><i class="fas fa-times"></i></a></span>
                    </div>
                    {{#each path.system.capacities as |capacity id|}}
                        <div class="gridcell gridcell-sm item capacity left color-default no-wrap" data-path-id="{{path._id}}" data-item-type="capacity" data-open-type="source" data-item-source="{{capacity.source}}" data-capacity-name="{{capacity.name}}" data-key="{{capacity.key}}">
                            {{#if capacity.selected}}
                                <a class="capacity-control capacity-checked" title="{{capacity.name}}"><i class="fas fa-square"></i></a>&nbsp;
                                <a class="capacity-control item-edit ellipsis">{{capacity.name}}</a>
                            {{else}}
                            <a class="capacity-control capacity-unchecked" title="{{capacity.name}}"><i class="far fa-square"></i>&nbsp;<a class="capacity-control item-edit ellipsis">{{capacity.name}}</a>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            {{/each}}
        </div>
    </div>

   {{#if (eq capacities.count 0)}}
       <p class="empty">{{localize "CO.label.noCapacitySelected"}}</p>
   {{else}}
       <li class="inventory-header flexrow">
           <div class="item-detail">{{localize "CO.label.rank"}}</div>
           <div class="item-controls">
               <a class="item-control item-create cof-compendium-pack" data-pack="capacities" data-open="0" title="{{localize 'COF.capacity.add'}}"><i class="fas fa-plus"></i>&nbsp;
                   {{localize "COF.ui.add"}}</a>
           </div>

       </li>
       {{#each capacities as |capacity id|}}
           {{log 'capacity' this}}
           <div class="item flexrow" data-item-type="capacity" data-open-type="embeddedId" data-item-id="{{capacity._id}}" data-item-source="{{capacity.source}}" data-capacity-name="{{capacity.name}}">
               <div class="item-image" style="background-image: url('{{capacity.img}}')"></div>
               <h4>{{capacity.name}}</h4>
               <div class="item-controls">
                   <a class="item-control item-edit" title="{{localize 'CO.label.edit'}}"><i class="fas fa-edit"></i></a>
                   <a class="item-control item-delete" title="{{localize "CO.label.delete"}}"><i class="fas fa-trash"></i></a>
               </div>
           </div>

       {{/each}}
   {{/if}}